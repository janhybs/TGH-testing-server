using System;

public class Example
{
   public static void Main()
   {
      string line;
      do { 
         line = Console.ReadLine();
         if (line != null) 
            Console.WriteLine(line);
      } while (line != null);   
   }
}



Program znak;
uses crt;
var veta : string;
begin
    Readln(veta);
    Writeln(veta);
    while Length(veta) <> 0 do
    begin
        Readln(veta);
        Writeln(veta);
    end;
    exit;
end.



